{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev",
    "dev:reset-deps": "rm -rf node_modules package-lock.json bun.lockb .next drizzle .turbo && bun i",
    "dev:hard-reset": "bun db:reset && bun dev",
    "lint": "next lint",
    "start": "next start",
    "db:init": "bun db:generate && bun db:migrate && bun db:push",
    "db:seed": "bun src/server/db/seed.ts",
    "db:stop": "./scripts/stop-database.sh",
    "db:nuke": "docker stop $(docker ps -a -q) || true && docker rm $(docker ps -a -q) || true && docker volume rm $(docker volume ls -q) || true",
    "db:create": "./scripts/start-database.sh && sleep 3 && bun db:init && bun db:seed",
    "db:reset": "bun db:stop || true && rm -rf drizzle || true && bun db:create",
    "stripe:init": "bun scripts/init-stripe.ts",
    "stripe:sync": "bun scripts/sync-stripe-products.ts",
    "stripe:nuke": "bun scripts/nuke-stripe.ts",
    "schedule-tasks": "bun scripts/schedule-tasks.ts"
  },
